{
  "name": "instagram-scheduler-server",
  "version": "1.0.0",
  "description": "Node.js server for scheduling Instagram posts",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop instagram-scheduler",
    "pm2:restart": "pm2 restart instagram-scheduler",
    "pm2:reload": "pm2 reload instagram-scheduler",
    "pm2:delete": "pm2 delete instagram-scheduler",
    "pm2:logs": "pm2 logs instagram-scheduler",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "node-cron": "^4.2.1",
    "pm2": "^6.0.11",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "instagram",
    "scheduler",
    "posts",
    "automation"
  ],
  "author": "",
  "license": "MIT"
}
